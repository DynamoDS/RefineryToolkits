      <DeployFolder Condition="'$(Configuration)' == 'Release'">$(SolutionDir)\..\release packages\$(ProjectName)</DeployFolder>
    </PropertyGroup>
    <!--Delete previously deployed packages first -->
    <RemoveDir Directories="$(DeployFolder)" />
